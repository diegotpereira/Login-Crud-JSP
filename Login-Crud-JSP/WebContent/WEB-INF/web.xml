<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Login-Crud-JSP</display-name>
    <welcome-file-list>   
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>LoginController</display-name>
    <servlet-name>Login</servlet-name>
    <servlet-class>br.com.java.controller.LoginController</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>LogoutController</display-name>
    <servlet-name>Logout</servlet-name>
    <servlet-class>br.com.java.controller.LogoutController</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>RegisterController</display-name>
    <servlet-name>Register</servlet-name>
    <servlet-class>br.com.java.controller.RegisterController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/Logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/Register</url-pattern>
  </servlet-mapping>
</web-app>